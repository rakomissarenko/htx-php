{
    "name": "feralonso/htx-php",
    "type": "library",
    "description": "Htx PHP API",
    "license": "MIT",
    "keywords": ["htx", "huobi"],
    "autoload": {
        "psr-4": {
            "Feralonso\\Htx\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feralonso\\Tests\\": "./tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "*",
        "ext-mbstring": "*",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
